<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="coreBundle\Entity\WebsiteZone" table="website_zone">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="name" type="string" column="name" length="150" nullable="false"/>
    <field name="zipCode" type="string" column="zip_code" length="5" nullable="false"/>
    <field name="activated" type="boolean" column="activated" nullable="false"/>
    <many-to-one field="department" target-entity="coreBundle\Entity\WebsiteDepartment">
      <join-columns>
        <join-column name="department_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
