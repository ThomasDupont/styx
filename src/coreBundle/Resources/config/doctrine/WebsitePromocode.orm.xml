<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="coreBundle\Entity\WebsitePromocode" table="website_promocode">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="createdAt" type="datetimetz" column="created_at" nullable="false"/>
    <field name="name" type="string" column="name" length="50" nullable="false"/>
    <field name="point" type="integer" column="point" nullable="true"/>
    <field name="activated" type="boolean" column="activated" nullable="false"/>
    <field name="dateBegin" type="datetimetz" column="date_begin" nullable="true"/>
    <field name="dateEnd" type="datetimetz" column="date_end" nullable="true"/>
    <field name="code" type="string" column="code" length="50" nullable="false"/>
  </entity>
</doctrine-mapping>
