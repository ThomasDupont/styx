<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="coreBundle\Entity\WebsiteGame" table="website_game">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="video" type="string" column="video" length="500" nullable="false"/>
    <field name="activated" type="boolean" column="activated" nullable="true"/>
    <field name="vote" type="integer" column="vote" nullable="true"/>
    <field name="title" type="string" column="title" length="32" nullable="true"/>
  </entity>
</doctrine-mapping>
