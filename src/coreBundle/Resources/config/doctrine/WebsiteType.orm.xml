<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="coreBundle\Entity\WebsiteType" table="website_type">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="false"/>
    <field name="color" type="string" column="color" length="7" nullable="false"/>
    <field name="binaryValue" type="string" column="binary_value" length="10" nullable="false"/>
  </entity>
</doctrine-mapping>
