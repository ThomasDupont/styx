<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="coreBundle\Entity\PostReward" table="post_reward">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="name" type="string" column="name" length="45" nullable="false"/>
    <field name="binaryValue" type="integer" column="binary_value" nullable="false"/>
    <field name="description" type="string" column="description" length="144" nullable="false"/>
    <field name="image" type="string" column="image" length="100" nullable="true"/>
  </entity>
</doctrine-mapping>
